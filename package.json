{
	"name": "demokratia",
	"version": "0.2.0",
	"description": "Voting system for IEEEsb elections.",
	"main": "server.js",
	"scripts": {
		"lint": "eslint . --ignore-path=.gitignore && ng lint",
		"ng": "ng",
		"watch-front": "ng build --watch --poll=1000",
		"watch-back": "nodemon --ignore dist/ --ignore frontend/",
